{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "outDir": "dist",
    "types": ["node", "express", "multer", "express-session"],
    "sourceMap": false
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts"]
}
