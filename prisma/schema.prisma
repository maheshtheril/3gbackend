/// ---------------- H M S  B I L L I N G  M O D E L S ----------------

model HmsTenant {
  id        String   @id @default(uuid())
  name      String
  createdAt DateTime @default(now())

  patients HmsPatient[]
  doctors  HmsDoctor[]
  services HmsService[]
  invoices HmsInvoice[]
  payments HmsPayment[]
  entries  HmsAccountingEntry[]
  messages HmsMessageLog[]
}

model HmsPatient {
  id        String    @id @default(uuid())
  tenantId  String
  tenant    HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  uhid      String?   @unique
  firstName String
  lastName  String?
  phone     String?
  email     String?
  createdAt DateTime  @default(now())

  invoices HmsInvoice[]
  messages HmsMessageLog[]
}

model HmsDoctor {
  id              String    @id @default(uuid())
  tenantId        String
  tenant          HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  name            String
  code            String?   @unique
  specialization  String?
  consultationFee Decimal   @default(0) @db.Decimal(10, 2)
  createdAt       DateTime  @default(now())
}

model HmsService {
  id        String    @id @default(uuid())
  tenantId  String
  tenant    HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  code      String?   @unique
  name      String
  rate      Decimal   @db.Decimal(12, 2)
  createdAt DateTime  @default(now())

  items HmsInvoiceItem[]
}

model HmsInvoice {
  id        String     @id @default(uuid())
  tenantId  String
  tenant    HmsTenant  @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  patientId String
  patient   HmsPatient @relation(fields: [patientId], references: [id])
  invoiceNo String     @unique
  subtotal  Decimal    @default(0) @db.Decimal(12, 2)
  discount  Decimal    @default(0) @db.Decimal(12, 2)
  tax       Decimal    @default(0) @db.Decimal(12, 2)
  total     Decimal    @default(0) @db.Decimal(12, 2)
  status    String     @default("UNPAID")
  createdAt DateTime   @default(now())

  items    HmsInvoiceItem[]
  payments HmsPayment[]
  entries  HmsAccountingEntry[] @relation("InvoiceEntries")
  messages HmsMessageLog[]      @relation("InvoiceMessages")
}


model HmsPayment {
  id         String     @id @default(uuid())
  tenantId   String
  tenant     HmsTenant  @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  invoiceId  String
  invoice    HmsInvoice @relation(fields: [invoiceId], references: [id], onDelete: Cascade)
  amount     Decimal    @db.Decimal(12, 2)
  mode       String
  reference  String?
  receivedAt DateTime   @default(now())
  createdAt  DateTime   @default(now())

  entries HmsAccountingEntry[] @relation("PaymentEntries")
}

model HmsAccountingEntry {
  id        String      @id @default(uuid())
  tenantId  String
  tenant    HmsTenant   @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  invoiceId String?
  invoice   HmsInvoice? @relation("InvoiceEntries", fields: [invoiceId], references: [id])
  paymentId String?
  payment   HmsPayment? @relation("PaymentEntries", fields: [paymentId], references: [id])
  account   String
  debit     Decimal     @default(0) @db.Decimal(14, 2)
  credit    Decimal     @default(0) @db.Decimal(14, 2)
  createdAt DateTime    @default(now())
}

model HmsMessageLog {
  id         String      @id @default(uuid())
  tenantId   String
  tenant     HmsTenant   @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  patientId  String?
  patient    HmsPatient? @relation(fields: [patientId], references: [id])
  invoiceId  String?
  invoice    HmsInvoice? @relation("InvoiceMessages", fields: [invoiceId], references: [id])
  channel    String
  to         String
  provider   String
  status     String
  providerId String?
  error      String?
  createdAt  DateTime    @default(now())
  updatedAt  DateTime    @updatedAt
}

/// ---------------- H M S  B I L L I N G  M O D E L S ----------------
/// ----------------- EXTENSIONS: Unified Invoice Item + Pharmacy Models -----------------

enum StockMoveType {
  RECEIPT
  ISSUE
  TRANSFER
  ADJUSTMENT
  SALE
  RETURN
  DAMAGED
}

model HmsTenant {
  id          String        @id @default(uuid())
  name        String
  products    HmsProduct[]
  warehouses  HmsWarehouse[]
  suppliers   HmsSupplier[]
  purchaseOrders HmsPurchaseOrder[]
  stockMoves  HmsStockMove[]
  stockLevels HmsStockLevel[]
  invoices    HmsInvoice[]
}

model HmsProduct {
  id         String   @id @default(uuid())
  tenantId   String
  tenant     HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  code       String   @unique
  barcode    String?  @unique
  name       String
  brand      String?
  category   String?
  uom        String?              
  packSize   Int?                 
  salePrice  Decimal? @db.Decimal(12,2)
  costPrice  Decimal? @db.Decimal(12,2)
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  batches      HmsProductBatch[]
  inventory    HmsStockLevel[]      
  invoiceItems HmsInvoiceItem[]   
  stockMoves   HmsStockMove[]
  purchaseLines HmsPurchaseLine[]

  @@index([name])
  @@index([code])
  @@index([barcode])
}

model HmsWarehouse {
  id        String   @id @default(uuid())
  tenantId  String
  tenant    HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  code      String   @unique
  name      String
  address   String?
  createdAt DateTime @default(now())

  locations HmsLocation[]
}

model HmsLocation {
  id          String   @id @default(uuid())
  tenantId    String
  tenant      HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  warehouseId String
  warehouse   HmsWarehouse @relation(fields: [warehouseId], references: [id], onDelete: Cascade)
  code        String   
  name        String?
  level       Int?     
  createdAt   DateTime @default(now())

  stockLevels HmsStockLevel[]

  @@index([warehouseId, code])
}

model HmsProductBatch {
  id          String   @id @default(uuid())
  productId   String
  product     HmsProduct @relation(fields: [productId], references: [id], onDelete: Cascade)
  batchNo     String
  manufacture DateTime?
  expiry      DateTime?
  costPrice   Decimal?  @db.Decimal(12,2)
  createdAt   DateTime  @default(now())

  stockMoves  HmsStockMove[]

  @@index([productId, batchNo])
  @@index([expiry])
}

model HmsStockLevel {
  id         String   @id @default(uuid())
  tenantId   String
  tenant     HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  productId  String
  product    HmsProduct @relation(fields: [productId], references: [id], onDelete: Cascade)
  locationId String
  location   HmsLocation @relation(fields: [locationId], references: [id], onDelete: Cascade)
  batchId    String?    
  batch      HmsProductBatch? @relation(fields: [batchId], references: [id])
  quantity   Int       @default(0)
  reserved   Int       @default(0)  
  createdAt  DateTime  @default(now())
  updatedAt  DateTime  @updatedAt

  @@unique([productId, locationId, batchId])
  @@index([productId])
  @@index([locationId])
}

model HmsStockMove {
  id             String   @id @default(uuid())
  tenantId       String
  tenant         HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  productId      String
  product        HmsProduct @relation(fields: [productId], references: [id])
  batchId        String?
  batch          HmsProductBatch? @relation(fields: [batchId], references: [id])
  fromLocationId String?
  fromLocation   HmsLocation? @relation("FromLoc", fields: [fromLocationId], references: [id])
  toLocationId   String?
  toLocation     HmsLocation? @relation("ToLoc", fields: [toLocationId], references: [id])
  qty            Int
  type           StockMoveType
  ref            String?   
  createdBy      String?
  createdAt      DateTime @default(now())

  @@index([productId])
  @@index([ref])
  @@index([fromLocationId])
  @@index([toLocationId])
}

model HmsSupplier {
  id        String @id @default(uuid())
  tenantId  String
  tenant    HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  name      String
  phone     String?
  email     String?
  address   String?
  createdAt DateTime @default(now())
}

model HmsPurchaseOrder {
  id         String   @id @default(uuid())
  tenantId   String
  tenant     HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  supplierId String?
  supplier   HmsSupplier? @relation(fields: [supplierId], references: [id])
  orderNo    String   @unique
  status     String   // DRAFT | ORDERED | RECEIVED | CANCELLED
  createdAt  DateTime @default(now())

  lines      HmsPurchaseLine[]
}

model HmsPurchaseLine {
  id          String   @id @default(uuid())
  tenantId    String
  tenant      HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  purchaseId  String
  purchase    HmsPurchaseOrder @relation(fields: [purchaseId], references: [id], onDelete: Cascade)
  productId   String
  product     HmsProduct @relation(fields: [productId], references: [id])
  qty         Int
  receivedQty Int @default(0)
  rate        Decimal @db.Decimal(12,2)
  batchNo     String?
  expiry      DateTime?
}

model HmsInvoice {
  id         String   @id @default(uuid())
  tenantId   String
  tenant     HmsTenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)
  invoiceNo  String   @unique
  date       DateTime @default(now())
  total      Decimal  @db.Decimal(12,2)
  items      HmsInvoiceItem[]
}

model HmsInvoiceItem {
  id          String   @id @default(uuid())
  invoiceId   String
  invoice     HmsInvoice @relation(fields: [invoiceId], references: [id], onDelete: Cascade)
  serviceId   String?
  productId   String?
  batchId     String?
  description String
  qty         Int       @default(1)
  rate        Decimal   @db.Decimal(12, 2)
  amount      Decimal   @db.Decimal(12, 2)

  service     HmsService?      @relation(fields: [serviceId], references: [id])
  product     HmsProduct?      @relation(fields: [productId], references: [id])
  batch       HmsProductBatch? @relation(fields: [batchId], references: [id])
}
